# environment variable
export PATH="/data3/users/xiezhiang/matlab/bin"$PATH

# delete ^M
:e ++ff=dos
:set ff=unix

find .|xargs grep x

MAC 增加颜色设置
# Tell ls to be colourful
export CLICOLOR=1
export LSCOLORS=Exfxcxdxbxegedabagacad

# Tell grep to highlight matches
export GREP_OPTIONS='--color=auto'
# from https://blog.csdn.net/wxqee/article/details/49406597 

# solarized设置变化 line 244
if ((has("gui_running") && g:solarized_degrade ==0) || has('termguicolors'))

"vim配置
"字符串编码
set encoding=utf-8
set fileencoding=utf-8
"set termencoding=utf-8

"行号，默认存储和换行
set nu
set autoindent
set nobackup

"语法高亮
syntax enable
syntax on
“作者：袁刚
”链接：https://www.zhihu.com/question/51713545/answer/288863269
if $COLORTERM == 'truecolor'
    set termguicolors
    colorscheme s
else
    set term=xterm
    set t_Co=256
    colorscheme solarized
endif

"缩进和自动补全
set tabstop=4
set expandtab
set shiftwidth=4
set wildmenu
inoremap <tab><space> <C-X><C-F>

